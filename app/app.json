{
  "expo": {
    "name": "lucidy",
    "slug": "lucidy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#022234",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "be.lorikku.lucidy",
      "versionCode": 1
    },
    "androidStatusBar": {
      "backgroundColor": "#011521"
 },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#022234"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "lucidy"
  }
}
